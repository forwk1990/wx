.wrapper-new-header {
    height: 50px;
    line-height: 50px;
    width: 100%;
    position: fixed;
    border-bottom: 1px solid #b3b3b3;
    font-size: 16px;
    color: black;
    z-index: 4;
    border-top: 1px solid #CCCCCC;
    background-color: #FFF;
    background: -webkit-linear-gradient(#FFFFFF, #DDDDDD); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(#FFFFFF, #DDDDDD); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(#FFFFFF, #DDDDDD); /* Firefox 3.6 - 15 */
    background: linear-gradient(#FFFFFF, #DDDDDD);
}

.title-logo {
    position: absolute;
    left: 0px;
    top: 0px;
    display: block;
    width: 40px;
    height: 42.5px;
    background: url("../images/title-logo.png") center center no-repeat;
    background-size: 40px 42.5px;
    float: left;
    margin-left: 10px;
}

.header-search {
    position: absolute;
    right: 0px;
    top: 15px;
    display: block;
    width: 20px;
    height: 20px;
    background: url("../images/search-new.png") center center no-repeat;
    background-size: 20px 20px;
    float: right;
    margin-right: 10px;
}

.search-lading-area {
    font-family: "Helvetica Neue", HelveticaNeue, "Helvetica-Neue", Helvetica, "BBAlpha Sans", sans-serif;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -o-user-select: none;
    user-select: none;
}

.search-landing-keywords {
    list-style-type: none;
    display: block;
    margin: 0;
    padding: 0;
    background: #F8F8F8;
}

.search-landing-keywords:after {
    clear: both;
    content: ".";
    height: 0;
    visibility: hidden;
    display: block;
    font-size: 0;
}

.search-landing-li {
    height: 34px;
    line-height: 34px;
    border-bottom: 1px solid #dedede;
    border-width: 0 0 1px 0;
    -webkit-border-image: url('data:image/gif;base64,R0lGODlhBAAEAIABAL+/v////yH/C1hNUCBEYXRhWE1QPD94cGFj…YFxYVFBMSERAPDg0MCwoJCAcGBQQDAgEAACH5BAEAAAEALAAAAAAEAAQAAAIFhBGpeVAAOw==') 2 0 stretch;
    -o-border-image1: url('data:image/gif;base64,R0lGODlhBAAEAIABAL+/v////yH/C1hNUCBEYXRhWE1QPD94cGFj…YFxYVFBMSERAPDg0MCwoJCAcGBQQDAgEAACH5BAEAAAEALAAAAAAEAAQAAAIFhBGpeVAAOw==') 2 0 round;
    border-image1: url('data:image/gif;base64,R0lGODlhBAAEAIABAL+/v////yH/C1hNUCBEYXRhWE1QPD94cGFj…YFxYVFBMSERAPDg0MCwoJCAcGBQQDAgEAACH5BAEAAAEALAAAAAAEAAQAAAIFhBGpeVAAOw==') 2 0 round;
}

.search-landing-li a {
    display: block;
    text-decoration: none;
    height: 34px;
    line-height: 34px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.search-landing-li a:after {
    clear: both;
    content: ".";
    height: 0;
    visibility: hidden;
    display: block;
    font-size: 0;
}

.search-landing-entry {
    float: left;
    color: #848689;
    font-size: 14px;
}

.search-magnifier-icon {
    margin-top: 9px;
    width: 16px;
    height: 16px;
}

.search-result-old-icon {
    display: block;
